{
  "name": "exile-test-platform",
  "private": true,
  "version": "5.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "build:dev": "cross-env VITE_COMPRESS=Y vite build",
    "build": "cross-env VITE_COMPRESS=Y vite build",
    "preview": "vite preview",
    "commit": "git add -A && git-cz",
    "lint:prettier": "prettier --write --list-different .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.**": [
      "prettier --check --ignore-unknown",
      "git add ."
    ]
  },
  "dependencies": {
    "@vue/shared": "^3.2.37",
    "@vueuse/core": "^9.1.0",
    "axios": "^0.27.2",
    "codemirror": "5.65.7",
    "dayjs": "^1.11.4",
    "echarts": "^5.3.3",
    "jsonlint-mod": "^1.7.6",
    "lodash": "^4.17.21",
    "tdesign-icons-vue-next": "^0.1.4",
    "tdesign-vue-next": "0.23.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "commitizen": "^4.2.4",
    "commitlint": "^16.2.1",
    "commitlint-config-cz": "^0.13.3",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "husky": "^8.0.1",
    "prettier": "2.5.1",
    "sass": "^1.54.3",
    "vite": "^3.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svg-icons": "^2.0.1"
  }
}
